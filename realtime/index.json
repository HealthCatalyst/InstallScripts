{
    "name": "Fabric Realtime",
    "version": "4.0.0.2",
    "secrets": {
        "password": [
            "mysqlrootpassword",
            "mysqlpassword",
            "certpassword",
            "rabbitmqmgmtuipassword"
        ],
        "value": [{
            "name": "certhostname",
            "valueFromSecret": {
                "name": "dnshostname",
                "namespace": "default"
            }
        }]
    },
    "resources": {
        "volumes": {
            "onprem": [
                "certificateserver.yaml",
                "mysqlserver.yaml",
                "rabbitmq-cert.yaml",
                "rabbitmq.yaml"
            ],
            "azure": [
                "certificateserver.yaml",
                "mysqlserver.yaml",
                "rabbitmq-cert.yaml",
                "rabbitmq.yaml"
            ]
        },
        "volumeclaims": [
            "certificateserver.yaml",
            "mysqlserver.yaml",
            "rabbitmq-cert.yaml",
            "rabbitmq.yaml"
        ],
        "pods": [
            "certificateserver.yaml",
            "mysqlserver.yaml",
            "interfaceengine.yaml",
            "rabbitmq.yaml"
        ],
        "services": {
            "cluster": [
                "certificateserver.yaml",
                "mysqlserver.yaml",
                "interfaceengine.yaml",
                "rabbitmq.yaml"
            ],
            "external": [
                "certificateserver.yaml",
                "rabbitmq.yaml",
                "interfaceengine.yaml"
            ]
        },
        "ingress": {
            "http": [
                "web.yaml",
                "rabbitmq.yaml",
                "interfaceengine.yaml"
            ],
            "tcp": [
                "mysqlserver.yaml",
                "interfaceengine.yaml",
                "rabbitmq.yaml"
            ]
        }
    }
}